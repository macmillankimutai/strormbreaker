#if($rangerName)
  <h2>Hello, $rangerName!</h2>
  <h3>Recent Sightings</h3>
  #if($recentSightings.size() > 0)
    <ul>
      #foreach($sighting in $recentSightings)
      <li><a href="/sightings/$sighting.getId()">Ranger: $sighting.getRangerName() at $sighting.getFormattedDate()</a></li>
      #end
    </ul>
  #else
    <h5>No recent sightings!</h5>
  #end
